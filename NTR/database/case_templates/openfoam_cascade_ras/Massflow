set title 'Massenstroeme'
set ylabel 'Massenstrom [kg/s]'
set xlabel 'Iteration'

listINLET=system('ls post*/flow*INLET*/*/surfaceFieldValue*.dat')
listOUTLET=system('ls post*/flow*OUTLET*/*/surfaceFieldValue*.dat')


plot for [file in listINLET] file using 1:($2)  title 'Inlet' with lines lt rgb "red", \
for [file in listOUTLET] file using 1:($2*-1)  title 'Outlet' with lines lt rgb "black"

#set term png
#set terminal png size 1024,768
#set output 'Massenstromverlauf.png'

pause 30 
reread

