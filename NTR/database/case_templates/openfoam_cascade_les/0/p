FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.042";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    INLET
    {
        type            waveTransmissive;
        gamma           1.4;
        value           uniform <var PINLET var>;

    }
    OUTLET
    {
        type            waveTransmissive;
        gamma           1.4;
	    value		    uniform <var POUTLET var>;
	    fieldInf        <var POUTLET var>;
        lInf            <var POUT_LINF var>;             //a measure of how far away the far-field condition should be
    }
    BLADE
    {
        type            zeroGradient;
    }

    Z_lower
    {
        type            cyclic;
    }

    Z_upper
    {
        type            cyclic;
    }

    Y_lower
    {
        type            cyclic;
    }

    Y_upper
    {
        type            cyclic;
    }


}
// ************************************************************************* //
